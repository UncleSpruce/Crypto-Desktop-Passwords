class Vault(object):
	'''
	classdocs
	'''

	def __init__(self, pList = []):
		'''
		Constructor
		'''
		if not isinstance(pPasswordTuple, PasswordList):
			raise TypeError("Argument passed into append function for PasswordList is not a PasswordTuple.")
		self.list = pList
		
	def append(self, pItem):
		self.list.append(pItem)
		
	def getList(self):
		return self.list
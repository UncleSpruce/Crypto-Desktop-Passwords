from primary.iCombiningMethodology import iCombiningMethodology

class BasicCombiner(iCombiningMethodology):
	'''
	classdocs
	'''
	def __init__(self, params):
		'''
		Constructor
		'''
		
	def combine(self, pList):
		total = 0
		for i in pList:
			total += i
		return total
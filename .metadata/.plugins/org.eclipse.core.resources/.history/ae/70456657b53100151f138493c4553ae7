import pickle

class PicklingMethodology(object):
	'''
	classdocs
	'''

	def __init__(self):
		'''
		Constructor
		'''
	def saveVault(self, pPasswordData, pFilename):
		text_file = open(pFilename, "wb")
		text_file.write(pPasswordData.createPickledVault())
		text_file.close()

	def getVaultFromFile(self, pFileName):
		return pickle.load(open(pFileName, 'rb'))
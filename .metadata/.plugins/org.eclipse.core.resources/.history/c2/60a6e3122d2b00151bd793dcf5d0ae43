class BasicJoinerAndCombiner(object):
	'''
	classdocs
	'''
	def __init__(self, pConverter, pJoiner, pCombiner):
		'''
		Constructor
		'''
		self.joiner = pJoiner
		self.combiner = pCombiner
		self.converter = pConverter
		
	def joinAndCombine(self, pPasswordList):
		lclList = []
		for i in pPasswordList:
			lclList.append(self.joiner())
		
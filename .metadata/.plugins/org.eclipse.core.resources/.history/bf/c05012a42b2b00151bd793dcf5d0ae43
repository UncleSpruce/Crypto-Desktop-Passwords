import iJoiningBehavior

class BasicStringJoiner(iJoiningBehavior):
	'''
	classdocs
	'''

	def __init__(self, params):
		'''
		Constructor
		'''
	
	def joinTwoStrings(self, pConverter, pString1, pString2):
	
		return pConverter.toInt(pString1) + pConverter.toInt(pString2)
	
	def stringToBigInt(self, pString):
		lclList = list(pString)
		
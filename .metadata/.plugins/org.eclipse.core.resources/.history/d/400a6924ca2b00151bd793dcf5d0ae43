from Crypto.Hash import SHA512
import hashlib
from primary.iHashMethodology import iHashMethodology

class BasicHash(iHashMethodology):
	'''
	classdocs
	'''
	def __init__(self):
		'''
		Constructor
		'''
		
	def compute(self, pArgument):
		lclHash = int(hashlib.sha512(str(pArgument)).hexdigest())
		return lclHash